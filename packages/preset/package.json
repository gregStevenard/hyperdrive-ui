{
  "name": "@hyperdrive-ui/panda-preset",
  "version": "0.0.1",
  "description": "Panda CSS Preset for Hyperdrive UI",
  "keywords": [],
  "author": "Gregoire Stevenard",
  "licence": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --watch",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/react": "^5.18.3",
    "effect": "^3.16.12"
  },
  "devDependencies": {
    "@pandacss/dev": "1.0.1",
    "tsup": "8.3.5"
  },
  "peerDependencies": {
    "@pandacss/dev": ">1.0.1"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    },
    "./anatomy/*": {
      "import": {
        "types": "./src/anatomy/*.ts",
        "default": "./src/anatomy/*.ts"
      },
      "require": {
        "types": "./dist/anatomy/*.d.cts",
        "default": "./dist/anatomy/*.cjs"
      }
    }
  }
}