{
  "name": "@hyperdrive-ui/panda-preset",
  "version": "0.0.1",
  "description": "Panda CSS Preset for Hyperdrive UI",
  "keywords": [],
  "author": "Gregoire Stevenard",
  "licence": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperdrive-ui/hyperdrive-ui.git",
    "directory": "packages/preset"
  },
  "bugs": {
    "url": "https://github.com/hyperdrive-ui/hyperdrive-ui/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "check-types": "tsc --noEmit",
    "release-it": "release-it",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@ark-ui/react": "^5.18.3",
    "effect": "^3.16.12"
  }, 
  "devDependencies": {
    "@pandacss/dev": "1.0.1",
    "tsup": "8.3.5",
    "@release-it/keep-a-changelog": "5.0.0",
    "clean-package": "2.2.0",
    "release-it": "17.10.0"
  },
  "peerDependencies": {
    "@pandacss/dev": "^1.0.1"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    },
    "./anatomy/*": {
      "import": {
        "types": "./src/anatomy/*.ts",
        "default": "./src/anatomy/*.ts"
      },
      "require": {
        "types": "./dist/anatomy/*.d.cts",
        "default": "./dist/anatomy/*.cjs"
      }
    }
  }
}